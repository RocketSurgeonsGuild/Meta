<Project>
  <PropertyGroup>
    <!-- This list needs to be in reverse order of the dependencies (leaf first) so that the changes cascade correctly. -->
    <IncludeRocketSurgeryHosting Condition="'$(IncludeRocketSurgeryHosting)' == 'false' and '$(IncludeRocketSurgeryAspNetCore)' == 'true'">true</IncludeRocketSurgeryHosting>

    <IncludeRocketSurgery Condition="'$(IncludeRocketSurgeryCore)' == 'false' and '$(IncludeRocketSurgeryFunctions)' == 'true'">true</IncludeRocketSurgery>
    <IncludeRocketSurgery Condition="'$(IncludeRocketSurgeryCore)' == 'false' and '$(IncludeRocketSurgeryHosting)' == 'true'">true</IncludeRocketSurgery>
    <IncludeRocketSurgery Condition="'$(IncludeRocketSurgeryCore)' == 'false' and '$(IncludeRocketSurgeryAspNetCore)' == 'true'">true</IncludeRocketSurgery>

    <IncludeRocketSurgeryAbstractions Condition="'$(IncludeRocketSurgeryAbstractions)' == 'false' and '$(IncludeRocketSurgeryCore)' == 'true'">true</IncludeRocketSurgeryAbstractions>
  </PropertyGroup>
  <Import Project="$(MSBuildThisFileDirectory)\abstractions.targets" />
  <Import Project="$(MSBuildThisFileDirectory)\aspnetcore.targets" />
  <Import Project="$(MSBuildThisFileDirectory)\core.targets" />
  <Import Project="$(MSBuildThisFileDirectory)\functions.targets" />
  <Import Project="$(MSBuildThisFileDirectory)\hosting.targets" />
</Project>
